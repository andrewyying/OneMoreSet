{
  "expo": {
    "name": "OneMoreSet",
    "slug": "onemoreset",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onemoreset.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "package": "com.onemoreset.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "plugins": [
      "expo-notifications",
      "expo-live-activity",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow OneMoreSet to access your photos so you can attach bug screenshots."
        }
      ],
      "./plugins/withLiveActivityFrequentUpdates"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "67ace0fe-979a-475d-806a-eaee0529fc43"
      }
    }
  }
}
